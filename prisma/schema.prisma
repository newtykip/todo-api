generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES")
}

model User {
  id              Int     @unique @default(autoincrement())
  username        String  @id @unique
  password        String
  refreshToken    String?
  refreshTokenExp Int?
}

model Todo {
  id      Int    @unique @default(autoincrement())
  ownerId Int?
  status  Int    @id @default(0)
  text    String
}
